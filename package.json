{
  "name": "fixit-web",
  "version": "1.3.1",
  "description": "React-based web app for managing work orders, invoices, and payments.",
  "author": {
    "name": "Trevor Anderson",
    "email": "trevor@nerdware.cloud"
  },
  "license": "LicenseRef-LICENSE",
  "repository": "https://github.com/Nerdware-LLC/fixit-web",
  "private": true,
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": "^10.0.0"
  },
  "type": "module",
  "scripts": {
    "start": "vite",
    "lint": "eslint src",
    "postlint": "npm run test:types",
    "storybook": "storybook dev -p 6006",
    "storybook:test": "test-storybook",
    "storybook:build": "storybook build",
    "test": "vitest run",
    "test:ci": "vitest run --coverage --bail=1 --silent",
    "test:update-snapshots": "vitest run --update",
    "test:types": "tsc --project tsconfig.json --noEmit --skipLibCheck",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "build:analyze-bundle": "source-map-explorer 'dist/assets/*.js*' --html bundleAnalysisResult.html",
    "preview": "vite preview",
    "gql:update-schema": "rover graph fetch fixit@current > fixit@current.graphql",
    "postgql:update-schema": "graphql-codegen-esm --config codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.9.4",
    "@emotion/react": "^11.11.1",
