    "apollo3-cache-persist": ">=0.14.1 <1",
    "axios": "^1.6.7",
    "chart.js": "^4.3.0",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "dayjs": "^1.11.8",
    "formik": "^2.4.2",
    "graphql": "^16.8.1",
    "jwt-decode": "^4.0.0",
    "lodash.merge": "^4.6.2",
    "lottie-react": "^2.4.0",
