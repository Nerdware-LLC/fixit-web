{
  "name": "fixit-web",
  "version": "1.2.2",
  "description": "Web app for managing work orders, invoices, and payments.",
  "author": {
    "name": "Trevor Anderson",
    "email": "trevor@nerdware.cloud"
  },
  "repository": "https://github.com/Nerdware-LLC/fixit-web",
  "private": true,
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.0.0"
  },
  "scripts": {
    "start": "craco start",
    "start:https": "HTTPS=true SSL_CRT_FILE=cert.pem SSL_KEY_FILE=key.pem craco start",
    "https:make-cert": "openssl req -new -newkey rsa:2048 -days 999 -nodes -x509 -subj '/C=/ST=/L=/O=/CN=' -keyout key.pem -out cert.pem",
    "build": "craco build",
    "build:analyze-size": "source-map-explorer 'build/static/js/*.js'",
    "test": "craco test --watchAll=false",
    "test:update-snapshots": "npm run test -- --updateSnapshot",
    "test:ci": "npm run test -- --ci --coverage --coverageDirectory='coverage' --reporters='default' --reporters='./.github/github_actions_reporter.cjs'",
    "eject": "craco eject",
    "rover:fetch": "rover graph fetch fixit@current > fixit@current.graphql",
    "gql:codegen": "graphql-codegen --config codegen.yaml",
    "gql:update-types": "npm run rover:fetch && npm run gql:codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@mui/styles": "^5.12.3",
    "@mui/x-data-grid": "^6.3.1",
    "@mui/x-date-pickers": "^6.3.1",
    "@sentry/react": "^7.51.2",
    "@sentry/tracing": "^7.51.2",
    "@stripe/react-stripe-js": "^1.16.5",
    "@stripe/stripe-js": "^1.52.0",
    "apollo3-cache-persist": "^0.14.1",
    "axios": "^0.27.2",
    "chart.js": "^4.3.0",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "dayjs": "^1.11.7",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "lodash.isequal": "^4.5.0",
    "lodash.merge": "^4.6.2",
    "lottie-react": "^2.4.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-number-format": "^5.1.4",
    "react-router-dom": "^6.11.1",
    "react-toastify": "^9.1.2",
    "react-virtuoso": "^4.3.6",
    "strip-ansi": "^7.0.1",
    "web-vitals": "^3.3.1",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@apollo/rover": "^0.14.0",
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "@faker-js/faker": "^8.0.0",
    "@graphql-codegen/cli": "^3.3.1",
    "@graphql-codegen/client-preset": "^3.0.1",
    "@graphql-codegen/typescript": "^3.0.4",
    "@serverless-guru/prettier-plugin-import-order": "^0.4.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/lodash.isequal": "^4.5.6",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.1.3",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-test-renderer": "^18.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "source-map-explorer": "^2.5.3",
    "type-fest": "^3.10.0",
    "typescript": "^5.0.4"
  }
}
