{
  "name": "fixit-web",
  "version": "1.2.2",
  "description": "React-based web app for Fixit users.",
  "author": {
    "name": "Trevor Anderson",
    "email": "trevor@nerdware.cloud"
  },
  "repository": "https://github.com/Nerdware-LLC/fixit-web",
  "private": true,
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "engines": {
    "node": "^16.0.0",
    "npm": "^8.0.0"
  },
  "scripts": {
    "start": "craco start",
    "start:https": "HTTPS=true SSL_CRT_FILE=cert.pem SSL_KEY_FILE=key.pem craco start",
    "https:make-cert": "openssl req -new -newkey rsa:2048 -days 999 -nodes -x509 -subj '/C=/ST=/L=/O=/CN=' -keyout key.pem -out cert.pem",
    "build": "craco build",
    "test": "craco test --watchAll=false",
    "test:update-snapshots": "npm run test -- --updateSnapshot",
    "test:ci": "npm run test -- --ci --coverage --coverageDirectory='coverage' --reporters='default' --reporters='./.github/github_actions_reporter.cjs'",
    "eject": "craco eject",
    "rover:fetch": "rover graph fetch fixit@current > fixit@current.graphql",
    "gql:codegen": "graphql-codegen --config codegen.yaml",
    "gql:update-types": "npm run rover:fetch && npm run gql:codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.7.10",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.13",
    "@mui/styles": "^5.11.13",
    "@mui/x-data-grid": "^5.17.25",
    "@mui/x-date-pickers": "^5.0.20",
    "@sentry/react": "^7.43.0",
    "@sentry/tracing": "^7.43.0",
    "@stripe/react-stripe-js": "^1.16.5",
    "@stripe/stripe-js": "^1.49.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "apollo3-cache-persist": "^0.14.1",
    "axios": "^0.27.2",
    "chart.js": "^4.2.1",
    "formik": "^2.2.9",
    "graphql": "^16.6.0",
    "jwt-decode": "^3.1.2",
    "lodash.isequal": "^4.5.0",
    "lottie-react": "^2.4.0",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-imask": "^6.4.3",
    "react-router-dom": "^6.9.0",
    "react-scripts": "^5.0.1",
    "react-test-renderer": "^18.2.0",
    "react-toastify": "^9.1.1",
    "react-virtuoso": "^4.1.0",
    "strip-ansi": "^7.0.1",
    "type-fest": "^3.6.1",
    "typescript": "^5.0.2",
    "web-vitals": "^3.3.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "^2.16.1",
    "@graphql-codegen/client-preset": "^1.2.3",
    "@graphql-codegen/typescript": "^2.8.5"
  }
}
