{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@ROOT/*": ["./*"],
      "@/*": ["./src/*"],
      "@app/*": ["./src/app/*"],
      "@cache/*": ["./src/app/apolloCache/reactiveVars/*"],
      "@components/*": ["./src/components/*"],
      "@graphql/*": ["./src/graphql/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@images/*": ["./src/images/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@routers/*": ["./src/routers/*"],
      "@services/*": ["./src/services/*"],
      "@tests": ["./src/__tests__"],
      "@tests/*": ["./src/__tests__/*"],
      "@types": ["./src/types"],
      "@types/*": ["./src/types/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
